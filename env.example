# LeanAgent environment variables example
# Copy this file to .env and edit as needed

# Data paths
LEANAGENT_DATA__ROOT_DIR="./data"
LEANAGENT_DATA__REPO_DIR="./data/repos"
LEANAGENT_DATA__CACHE_DIR="./data/cache"

# Retrieval settings
LEANAGENT_RETRIEVAL__EMBEDDING_MODEL="text-embedding-ada-002"
LEANAGENT_RETRIEVAL__K="10"
LEANAGENT_RETRIEVAL__SIMILARITY_THRESHOLD="0.7"

# Prover settings
LEANAGENT_PROVER__MODEL="gpt-4"
LEANAGENT_PROVER__TEMPERATURE="0.7"
LEANAGENT_PROVER__MAX_TOKENS="8192"

# Database settings
LEANAGENT_DB__PATH="./data/db"
LEANAGENT_DB__CHUNK_SIZE="1000" 